#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Code/RecordImprovement/chuxiuStudyNotes/node_modules/conventional-commits-parser/node_modules:/mnt/d/Code/RecordImprovement/chuxiuStudyNotes/node_modules:/mnt/d/Code/RecordImprovement/node_modules:/mnt/d/Code/node_modules:/mnt/d/node_modules:/mnt/d/Code/RecordImprovement/chuxiuStudyNotes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Code/RecordImprovement/chuxiuStudyNotes/node_modules/conventional-commits-parser/node_modules:/mnt/d/Code/RecordImprovement/chuxiuStudyNotes/node_modules:/mnt/d/Code/RecordImprovement/node_modules:/mnt/d/Code/node_modules:/mnt/d/node_modules:/mnt/d/Code/RecordImprovement/chuxiuStudyNotes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-commits-parser/cli.js" "$@"
else
  exec node  "$basedir/../conventional-commits-parser/cli.js" "$@"
fi
